# LangSmith API Key (required for LangGraph)
LANGSMITH_API_KEY=lsv2_your_langsmith_api_key_here

# Google Gemini API Key (required for the LLM)
GOOGLE_API_KEY=your_google_gemini_api_key_here

# ElevenLabs API Key (required for text-to-speech)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Unsplash Access Key (required for image search)
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Kling providers
# Set provider to either 'replicate' or 'fal'. Defaults to 'replicate'
KLING_PROVIDER=replicate

# Replicate API key (if KLING_PROVIDER=replicate)
REPLICATE_API_TOKEN=your_replicate_api_token_here
# Optional: override the Replicate model ID
KLING_REPLICATE_MODEL=kwaivgi/kling-v2.1-master

# fal.ai API key (if KLING_PROVIDER=fal)
FAL_KEY=your_fal_api_key_here
# Optional: override the fal endpoint slug
KLING_FAL_ENDPOINT=kwaivgi/kling-v2.1-master
