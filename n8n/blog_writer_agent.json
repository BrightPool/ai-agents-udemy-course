      "credentials": {
        "openAiApi": {
          "id": "aEwEH5APyXx6cq8g",
          "name": "OpenAi account"
        }
      }
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "section_content_field",
              "name": "section_content",
              "value": "={{ $json.message.content || $json.content || 'Section content not available' }}",
              "type": "string"
            }
          ]
        },
        "options": {}
      },
      "id": "1e4ab7b1-8952-48a6-9675-249d398b0d01",
