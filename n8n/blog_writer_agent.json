      ]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "context_field",
              "name": "retrieved_context",
              "value": "={{ [\"Previous company writing about \" + $('Set Topic').first().json.topic + \": This technology has shown significant impact in enterprise environments.\", \"From our research on \" + $json.section_title.toLowerCase() + \": Key considerations include scalability, user adoption, and ROI measurement.\", \"Company style guide: Use practical, no-hype tone. Focus on actionable insights and real-world applications.\", \"Related case study: Implementation typically takes 3-6 months with proper planning and stakeholder buy-in.\"] }}",
              "type": "array"
            },
            {
              "id": "section_index_field",
              "name": "section_index",
              "value": "={{ $itemIndex }}",
              "type": "number"
            }
          ]
        },
        "options": {}
      },
      "id": "8e08e2a8-0208-4d00-bcf4-169da68363fe",
      "name": "Add Vector Context",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.4,
      "position": [
        1344,
        368
      ]
    },
    {
      "parameters": {
        "modelId": {
          "__rl": true,
          "value": "gpt-4.1-mini",
          "mode": "list",
          "cachedResultName": "GPT-4.1-MINI"
        },
        "messages": {
          "values": [
            {
              "content": "=You are a marketing blog writer. Write a focused section with context from prior company writing.\n\nTopic: {{ $('Set Topic').first().json.topic }}\nSection Title: {{ $('Split Outline to Items').item.json.section_title }}\n\nRetrieved Context:\n{{ $json.retrieved_context.join('\\n- ') }}\n\nWrite a focused section (3-6 paragraphs, concise) for this blog post. Use the retrieved context for facts and maintain a practical, no-hype tone. Make it actionable and specific to the topic.\n\nReturn only the section content, no title or extra formatting."
            }
          ]
        },
        "options": {
          "maxTokens": 800,
          "temperature": 0.8
        }
      },
      "id": "c2ab9d50-3572-4a7c-a57c-a75daf408998",
      "name": "Write Sections",
